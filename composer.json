{
    "name": "aoeng/laravel-amap",
    "description": "高德地图接口",
    "keywords": [
        "amap",
        "gaode",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dang",
            "email": "yofengge@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=8.0"
    },
    "autoload": {
