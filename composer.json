            "Aoeng\\Laravel\\Amap\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aoeng\\Laravel\\Amap\\AmapServiceProvider"
            ],
            "aliases": {
                "Amap": "Aoeng\\Laravel\\Amap\\Facades\\Amap"
            }
        }
    }
}
